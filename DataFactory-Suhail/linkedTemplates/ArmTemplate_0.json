		{
			"name": "[concat(parameters('factoryName'), '/Combo_ABC')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Execute PipelineA",
						"type": "ExecutePipeline",
						"dependsOn": [],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "pipelineA",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"WaitA": {
									"value": "@pipeline().parameters.A",
									"type": "Expression"
								}
							}
						}
					},
					{
						"name": "Execute PipelineB",
						"type": "ExecutePipeline",
						"dependsOn": [],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "pipelineB",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"WaitB": {
									"value": "@pipeline().parameters.B",
									"type": "Expression"
								}
							}
						}
					},
					{
						"name": "Execute Pipelinec",
						"type": "ExecutePipeline",
						"dependsOn": [
							{
								"activity": "Execute PipelineA",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "Execute PipelineB",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "pipelineC",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"WaitC": {
									"value": "@pipeline().parameters.C",
									"type": "Expression"
								}
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"A": {
						"type": "int"
					},
					"B": {
						"type": "int"
					},
					"C": {
						"type": "int"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/pipelineA')]",
				"[concat(variables('factoryId'), '/pipelines/pipelineB')]",
				"[concat(variables('factoryId'), '/pipelines/pipelineC')]"
